# Example environment variables for development
# Copy this to .env and set real values (do NOT commit .env to git)

# Database
POSTGRES_PASSWORD=changeme
DATABASE_URL=postgresql://cms_user:${POSTGRES_PASSWORD}@postgres:5432/production_cms

# App secrets (use secure random values in production)
JWT_SECRET=please_change_this_to_a_secure_random_string_at_least_32_chars
SESSION_SECRET=please_change_this_to_a_secure_random_string_at_least_32_chars

# pgAdmin (development only)
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin123

# Biscuit root key (if used)
BISCUIT_ROOT_KEY=replace_with_your_biscuit_root_key
